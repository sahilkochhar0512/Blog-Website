<%- include("partials/header"); %>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="titletext">Title</label>
    <input type="text" name="title" class="form-control" id="titletext" placeholder="Enter Title">
  </div>
  <div class="form-group">
    <label for="posttext">Post</label>
    <textarea type="text" name="message" cols="40" rows="5" class="form-control" id="posttext" placeholder="Start a Post"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>



<%- include("partials/footer"); %>